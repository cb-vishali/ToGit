
package summationofindices;
import java.util.*;
/**
 *
 * @author cb-vishali
 */
public class SummationOfIndices {

    public static void main(String[] args) {
        
        String input="java";
        int res=0;
        char arr[]=input.toCharArray();
        for(int i=0;i<arr.length;i++)
        {
            int x=Character.getNumericValue(arr[i])-9;
            res=res+x;
        }
        System.out.println(res);
         }
   }
